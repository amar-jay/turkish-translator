{
  "name": "turkish-translator",
  "version": "1.0.3",
  "description": "This is a tureng translator",
  "main": "lib/crawler.js",
  "types": "lib/crawler.d.ts",
  "scripts": {
    "watch": "tsc --watch",
    "start": "node lib/crawler.js",
    "format": "prettier --write \"src/**/*.ts\" \"lib/**/*.js\"",
    "lint": "eslint --ext .js,.ts src lib",
    "prepublishOnly": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "files": [
    "lib/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/amar-jay/turkish-translator.git"
  },
  "keywords": [
    "tureng",
    "translate",
    "english",
    "french",
    "turkish",
    "spanish",
    "german",
    "dictionary"
  ],
  "author": "Amar Jay",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/amar-jay/turkish-translator/issues"
  },
  "homepage": "https://github.com/cuneytdalan/turkish-translator#readme",
  "devDependencies": {
    "@types/crawler": "^1.2.0",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "eslint": "^8.8.0",
    "eslint-plugin-import": "^2.25.4",
    "prettier": "^2.1.2",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "@types/node": "^14.14.3",
    "crawler": "^1.3.0",
    "nodemon": "^2.0.6"
  }
}
